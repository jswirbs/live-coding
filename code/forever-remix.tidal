  d8 $ n (slow 8 $ "e fs e e") # octave 3 # s "superhammond" # voice 1 # gain 1 # sustain 1.6
  -- d9 CRASH
  -- d10 $ loopAt 8 $ s "DJ_DAVE_castles" # n 14 # gain 0.58 # hcutoff 3500

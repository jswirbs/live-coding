  d8 $ slice 32 (slow 2 $ "<14*4 10*4 8*4 7*4>") $ loopAt 16 $ s "DANNY_blue" # n 1 # speed "-1" # djf 0.7

do
  d1 $ s "techno_kicks*4" # n 0 # gain 1.05

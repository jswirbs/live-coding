setcps(132/60/4)


d3 $ chop 16 $ loopAt 16 $ s "DANNY_blue" # n 0
-- normalish speed
d3 $ chop 8 $ loopAt 8 $ s "DANNY_blue" # n 0
d3 $ chop 8 $ loopAt 8 $ s "DANNY_blue" # n 1

d3 $ gap 1 $ loopAt 16 $ s "DANNY_blue" # n 0
d3 $ gap 2 $ loopAt 16 $ s "DANNY_blue" # n 0

d3 $ gap 4 $ loopAt 16 $ s "DANNY_blue" # n 0

d4 $ s "XORE_hats*8" # n 16 # cutoff 2000 # room 0.4




do

d3 $ loopAt 1 $ s "DANNY_blue" # n 4 # djf 0.82

do
  d3 $ loopAt 1 $ s "DANNY_blue" # n 4
  d4 $ ghost $ s "[~ XORE_hats*2]*4" # n 4 # cutoff 7500 # room 0.25
  d5 $ (0.125 ~>) $ gap 8 $ slice 2 "<0 1>" $ loopAt 2 $ s "DANNY_blue" # n 3 # cutoff 2500

do
  d2 $ s "SOPHIE_subs*8" # n 5 # distort 0.2
  d5 $ (0.125 ~>) $ gap 8 $ slice 2 "<0 1>" $ loopAt 2 $ s "DANNY_blue" # n 3 # crush 6.5 -- # cutoff 2000
  d6 $ ghost $ s "[~ XORE_hats]*4" # n 4 # distort 0.2 # gain 0.9


xfadeIn 6 4 silence
d7 $ slice 32 "5*2" $ loopAt 16 $ s "DANNY_blue" # n 1 # speed "-1" # djf 0.7
-- d7 $ slice 32 "5*4" $ loopAt 8 $ s "DANNY_blue" # n 1 # speed "-1" # djf 0.7

do

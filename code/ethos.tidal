d5 $ (0.125 ~>) $ slice 16 (slow 2 $ "<0*8 4*8 14*8 13*8>") $ loopAt 2 $ s "DANNY_blue" # n 2 # cutoff 2000
d5 $ (0.125 ~>) $ slice 16 (slow 2 $ "<0 4 14 13>*8") $ loopAt 2 $ s "DANNY_blue" # n 2 # cutoff 2000


